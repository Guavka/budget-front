#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/Projects/Vue/personal_budget_app/services/front/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/e/Projects/Vue/personal_budget_app/services/front/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@antfu+ni@0.18.8/node_modules/@antfu/ni/bin/nun.mjs" "$@"
else
  exec node  "$basedir/../../../../../@antfu+ni@0.18.8/node_modules/@antfu/ni/bin/nun.mjs" "$@"
fi
